<?xml version="1.0" encoding="utf-8" ?>
<Policy xmlns="http://psl.cs.columbia.edu/discus2/Policy" 
		xmlns:all="http://psl.cs.columbia.edu/discus2/All"
		xmlns:rp="http://psl.cs.columbia.edu/discus2/RequiresParticipants"
		xmlns:rt="http://psl.cs.columbia.edu/discus2/RequiresTopic">
	<all:All>
		<rp:RequiresParticipants>
			<rp:Participant>joe</rp:Participant>
			<rp:Participant>bob</rp:Participant>
		</rp:RequiresParticipants>
		<Some xmlns="http://psl.cs.columbia.edu/discus2/Some">
			<min>3</min>
			<rt:RequiresTopic IsRegex="true">
				<rt:Topic>your topic|topic</rt:Topic>
			</rt:RequiresTopic>
			<rt:RequiresTopic IsRegex="true">
				<rt:Topic>foobar</rt:Topic>
			</rt:RequiresTopic>
			<rt:RequiresTopic>
				<rt:Topic>My Topic</rt:Topic>
			</rt:RequiresTopic>
		</Some>
	</all:All>
</Policy>

